<UserControl x:Class="Bska.Client.UI.Views.AccountingView.AccountCodingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Bska.Client.UI.Views.AccountingView"
      mc:Ignorable="d"
       x:Name="accountDocPage" FontFamily="{StaticResource PrimaryFontFamily}" FlowDirection="RightToLeft"
      xmlns:UCA="clr-namespace:Bska.Client.UI.UserControlls.AccountDocView">
    <Grid>
        <Border Style="{StaticResource BorderStyle}" BorderBrush="{StaticResource BorderBrush}" Margin="2">
            <Grid>
                <UCA:AccountDocCodingTree Margin="5,10,5,5"/>
            </Grid>
        </Border>
    </Grid>
</UserControl>
