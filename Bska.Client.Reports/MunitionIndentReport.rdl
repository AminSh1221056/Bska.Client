    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>recivedIndentDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AType">
            <Value>=Parameters!AType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@isGrouping">
            <Value>=Parameters!isGrouping.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@queryString">
            <Value>=Parameters!queryString.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @isGrouping=1
SELECT
  Count(Person.SubOrder.SubOrderId) as NCount
,0 as State
  ,Person.OrderDetails.StuffName
  ,Person.OrderDetails.StuffType
  ,GETDATE() AS OrderDate
  ,u.Name as UName
  ,Sum(Person.SubOrder.Num) as NSum
FROM
  Person.OrderDetails
  INNER JOIN Person.SubOrder
    ON Person.OrderDetails.OrderDetialsId = Person.SubOrder.OrderDetailsId
  INNER JOIN Person.[Order]
    ON Person.OrderDetails.OrderId = Person.[Order].OrderId
inner join Production.Unit as u on u.UnitId=Person.SubOrder.UnitId
Where Person.SubOrder.Type=2 And Person.OrderDetails.StuffType In (@AType) AND (CONVERT(varchar(10), Person.SubOrder.SubOrderId)=@queryString OR Person.OrderDetails.StuffName LIKE  ''+@queryString+'%' Or  @queryString is Null)
Group By Person.OrderDetails.StuffName, Person.OrderDetails.StuffType,
u.Name
Else
SELECT
  Person.SubOrder.SubOrderId as NCount
,Person.SubOrder.State
  ,Person.OrderDetails.StuffName
  ,Person.OrderDetails.StuffType
  ,Person.[Order].OrderDate
  ,u.Name as UName
  ,Person.SubOrder.Num as NSum
FROM
  Person.OrderDetails
  INNER JOIN Person.SubOrder
    ON Person.OrderDetails.OrderDetialsId = Person.SubOrder.OrderDetailsId
  INNER JOIN Person.[Order]
    ON Person.OrderDetails.OrderId = Person.[Order].OrderId
inner join Production.Unit as u on u.UnitId=Person.SubOrder.UnitId
Where Person.SubOrder.Type=2 And Person.OrderDetails.StuffType In (@AType) AND (CONVERT(varchar(10), Person.SubOrder.SubOrderId)=@queryString OR Person.OrderDetails.StuffName LIKE  ''+@queryString+'%' Or  @queryString is Null)</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
              <ColumnExpression Schema="Person" ColumnOwner="SubOrder" ColumnName="SubOrderId" />
              <ColumnExpression Schema="Person" ColumnOwner="OrderDetails" ColumnName="StuffName" />
              <ColumnExpression Schema="Person" ColumnOwner="OrderDetails" ColumnName="StuffType" />
              <ColumnExpression Schema="Person" ColumnOwner="Order" ColumnName="OrderDate" />
              <ColumnExpression Schema="Person" ColumnOwner="SubOrder" ColumnName="UnitId" />
              <ColumnExpression Schema="Person" ColumnOwner="SubOrder" ColumnName="Num" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="OrderDetails" Schema="Person" />
              <Join Type="INNER JOIN" ColumnOwner="SubOrder" Schema="Person">
                <On>
                  <From ColumnOwner="OrderDetails" Schema="Person" ColumnName="OrderDetialsId" />
                  <To ColumnOwner="SubOrder" Schema="Person" ColumnName="OrderDetailsId" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Order" Schema="Person">
                <On>
                  <From ColumnOwner="OrderDetails" Schema="Person" ColumnName="OrderId" />
                  <To ColumnOwner="Order" Schema="Person" ColumnName="OrderId" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
