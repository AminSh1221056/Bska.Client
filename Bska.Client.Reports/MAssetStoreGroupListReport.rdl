  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>storeGroupDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@queryLevel">
            <Value>=Parameters!queryLevel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stdId">
            <Value>=Parameters!stdId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AType">
            <Value>=Parameters!AType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@fromDate">
            <Value>=Parameters!fromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@toDate">
            <Value>=Parameters!toDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@queryString">
            <Value>=Parameters!queryString.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@storeId">
            <Value>=Parameters!storeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @queryLevel = 1 SELECT         p.Name, Sum(Num), Type, u.Name as UName
                                                                                               FROM            EmployeeResources.MovableAsset AS p INNER JOIN
                                                                                                                        EmployeeResources.Location AS l ON l.AssetId = p.AssetId INNER JOIN Production.Unit as u
	 on p.UnitId=u.UnitId
                                                                                               WHERE     (l.[Status]=4 Or l.[Status]=10) AND l.StoreAddressId = @stdId  AND  p.[Type] IN (@AType)
Group BY p.Name,Type,u.Name
Else IF @queryLevel = 2 WITH Parent(refid, Sno, Level) AS
                                  (SELECT        m.StoreDesignId, m.NodeId, 0 AS Level
                                     FROM            Production.StoreDesign m
                                     WHERE        m.StoreDesignId = @stdId
                                     UNION ALL
                                     SELECT        t .StoreDesignId, t .NodeId, Level + 1
                                     FROM            Production.StoreDesign t INNER JOIN
                                                              Parent p ON p.refid = t .NodeId)
                             SELECT        p.Name, Sum(Num), Type, u.Name as UName
                                   FROM            EmployeeResources.MovableAsset AS p INNER JOIN
                                                           EmployeeResources.Location AS l ON l.AssetId = p.AssetId INNER JOIN Production.Unit as u
	 on p.UnitId=u.UnitId INNER JOIN
                                                            Parent pa ON pa.refid = l.StoreAddressId
                                   WHERE    (l.[Status]=4 Or l.[Status]=10)  AND    pa.refid = l.StoreAddressId  AND  p.[Type] IN (@AType)
 And (p.InsertDate between @fromDate and @toDate Or @fromDate Is null or @toDate is Null) AND (CONVERT(varchar(10), p.Label)=@queryString OR p.Name LIKE  ''+@queryString+'%' Or  @queryString is Null) Group BY p.Name,Type,u.Name
ELSE
    SELECT        p.Name, Sum(Num), Type, u.Name as UName
                                                                                                                                                                                         FROM            EmployeeResources.MovableAsset p INNER JOIN
                                                                                                                                                                                                                EmployeeResources.Location AS l ON l.AssetId = p.AssetId INNER JOIN Production.Unit as u
	 on p.UnitId=u.UnitId
                                                                                                                                                                                         WHERE       (l.[Status]=4 Or l.[Status]=10) ANd (l.StoreId = @storeId ) AND  p.[Type] IN (@AType)
 And (p.InsertDate between @fromDate and @toDate Or @fromDate Is null or @toDate is Null) AND (CONVERT(varchar(10), p.Label)=@queryString OR p.Name LIKE  ''+@queryString+'%' Or  @queryString is Null) Group BY p.Name,Type,u.Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Type">
