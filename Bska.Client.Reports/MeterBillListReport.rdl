  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>meterBillDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@mType">
            <Value>=Parameters!mType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@queryString">
            <Value>=Parameters!queryString.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF 2500 IN (@mType)  
SELECT
  EmployeeResources.Meter.Name AS [Meter Name]
,EmployeeResources.MeterBill.Type
  ,Production.Building.Name AS [Building Name]
  ,EmployeeResources.MeterBill.[Year]
  ,EmployeeResources.MeterBill.Mounth
  ,EmployeeResources.MeterBill.OtehrCost
  ,EmployeeResources.MeterBill.DebtorCost
  ,EmployeeResources.MeterBill.TaxCost
  ,EmployeeResources.MeterBill.CostEra
  ,EmployeeResources.MeterBill.PayDate
  ,EmployeeResources.MeterBill.PayRecognition
  ,EmployeeResources.MeterBill.BillRecognition
,EmployeeResources.MeterBill.DNum1
,EmployeeResources.MeterBill.DNum2
,EmployeeResources.MeterBill.DNum3
,EmployeeResources.MeterBill.DNum4
,EmployeeResources.MeterBill.DNum5
,EmployeeResources.MeterBill.InsertDate
FROM
  EmployeeResources.Meter
  INNER JOIN Production.Building
    ON EmployeeResources.Meter.BuildingId = Production.Building.BuildingId
  INNER JOIN EmployeeResources.MeterBill
    ON EmployeeResources.Meter.AssetId = EmployeeResources.MeterBill.AssetId
Where EmployeeResources.Meter.Name LIKE  ''+@queryString+'%' Or  @queryString is Null
Else
SELECT
  EmployeeResources.Meter.Name AS [Meter Name]
 ,EmployeeResources.MeterBill.Type
  ,Production.Building.Name AS [Building Name]
  ,EmployeeResources.MeterBill.[Year]
  ,EmployeeResources.MeterBill.Mounth
  ,EmployeeResources.MeterBill.OtehrCost
  ,EmployeeResources.MeterBill.DebtorCost
  ,EmployeeResources.MeterBill.TaxCost
  ,EmployeeResources.MeterBill.CostEra
  ,EmployeeResources.MeterBill.PayDate
  ,EmployeeResources.MeterBill.PayRecognition
  ,EmployeeResources.MeterBill.BillRecognition
,EmployeeResources.MeterBill.DNum1
,EmployeeResources.MeterBill.DNum2
,EmployeeResources.MeterBill.DNum3
,EmployeeResources.MeterBill.DNum4
,EmployeeResources.MeterBill.DNum5
,EmployeeResources.MeterBill.InsertDate
FROM
  EmployeeResources.Meter
  INNER JOIN Production.Building
    ON EmployeeResources.Meter.BuildingId = Production.Building.BuildingId
  INNER JOIN EmployeeResources.MeterBill
    ON EmployeeResources.Meter.AssetId = EmployeeResources.MeterBill.AssetId
Where EmployeeResources.MeterBill.Type In (@mType) And (EmployeeResources.Meter.Name LIKE  ''+@queryString+'%' Or  @queryString is Null)</CommandText>
